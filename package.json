{
  "name": "node-api-bootstrap",
  "version": "1.0.0",
  "description": "NodeJS Express bootstrap application to quick start API development process",
  "main": "index.js",
  "keywords": "nodejs, api, node api bootstrap, node api quickstart",
  "scripts": {
    "start": "node index.js",
    "prod": "nodemon index.js --ENV production",
    "dev": "nodemon index.js --ENV development",
    "devcluster": "nodemon index.js --ENV development --CLUSTERED true",
    "devseed": "nodemon index.js --ENV development --SEED true",
    "prodseed": "nodemon index.js --ENV production --SEED true",
    "test": "./node_modules/.bin/mocha --reporter spec"
  },
  "engines": {
    "node": "8.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/himanshuakatalli/node-api-bootstrap.git"
  },
  "author": "Team IMS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/himanshuakatalli/node-api-bootstrap/issues"
  },
  "homepage": "https://github.com/himanshuakatalli/node-api-bootstrap#readme",
  "dependencies": {
    "bcrypt": "1.0.2",
    "body-parser": "1.17.2",
    "chai": "4.1.0",
    "cors": "2.8.3",
    "dotenv": "4.0.0",
    "express": "4.15.3",
    "jsonwebtoken": "7.4.1",
    "lodash": "4.17.4",
    "mailgun-js": "0.11.3",
    "minimist": "1.2.0",
    "mocha": "3.4.2",
    "mongoose": "4.10.8",
    "oauth2orize": "1.8.0",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-http-bearer": "1.0.1",
    "passport-local": "1.0.0",
    "passport-oauth2": "1.4.0",
    "passport-oauth2-client-password": "0.1.2",
    "request": "2.81.0"
  },
  "devDependencies": {
    "nodemon": "1.11.0"
  }
}
